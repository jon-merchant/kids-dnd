        <template><![CDATA[<div class="clearfix" ng-repeat="character in c.data.guildCharList track by $index">
  <div class="col-sm-4">
    <div ng-click="c.openModal('guildMemberInfo','sm',character)" ng-if="character.image.url" class="margin-right-15 inline-block avatar avatar-sm" style="background-image:url({{character.image.url}})"></div>
